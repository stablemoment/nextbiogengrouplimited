{
    "framework": "next",
    "functions": {
      "start": "next start"
    },
    "builds": [
      { "src": "public/**/*", "use": "@vercel/static" },
      { "src": "next.config.js", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "/" }
    ],
    "generateCommand": "next-sitemap",
    "lintCommand": "next lint",
    "postbuild": "next-sitemap",
    "cleanUrls": true
  }
  